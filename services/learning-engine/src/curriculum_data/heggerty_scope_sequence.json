      "intervention_threshold": 0.50
    },
    "weeks_6_10": {
      "focus": "Onset-rime and initial sound isolation",
      "mastery_target": 0.75,
      "intervention_threshold": 0.55
    },
    "weeks_11_15": {
      "focus": "CVC blending and segmentation",
      "mastery_target": 0.80,
      "intervention_threshold": 0.60
    },
    "weeks_16_20": {
      "focus": "Complex blending and phoneme addition",
      "mastery_target": 0.75,
      "intervention_threshold": 0.55
    },
    "weeks_21_25": {
      "focus": "Phoneme deletion",
      "mastery_target": 0.70,
      "intervention_threshold": 0.50
